{% extends 'base.html.twig' %}

{% block title %}DayPart index{% endblock %}

{% block body %}
    <h1>DayPart index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for day_part in day_parts %}
            <tr>
                <td>{{ day_part.id }}</td>
                <td>
                    <a href="{{ path('day_part_show', {'id': day_part.id}) }}">show</a>
                    <a href="{{ path('day_part_edit', {'id': day_part.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('day_part_new') }}">Create new</a>
{% endblock %}
